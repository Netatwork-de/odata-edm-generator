{
  "name": "@netatwork/odata-edm-generator",
  "version": "1.0.0-3",
  "description": "OData EDM generator",
  "typings": "dist/types/decorators",
  "main": "dist/cjs/api/decorators.js",
  "module": "dist/esm/decorators.js",
  "bin": {
    "gen-edm": "dist/cjs/cli/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --cache",
    "prebuild": "rimraf dist && npm run lint",
    "build": "tsc && tsc -p tsconfig.esm.json",
    "dev": "tsc -w --preserveWatchOutput",
    "test": "nyc --nycrc-path ./tests/nyc.config.js mocha --config ./tests/.mocharc.js",
    "test:dev": "cross-env DEV=true nyc --nycrc-path ./tests/nyc.config.js mocha --config ./tests/.mocharc.js --bail"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Netatwork-de/odata-edm-generator.git"
  },
  "keywords": [
    "odata",
    "edm",
    "generator"
  ],
  "author": "Net at Work GmbH",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Netatwork-de/odata-edm-generator/issues"
  },
  "homepage": "https://github.com/Netatwork-de/odata-edm-generator#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@netatwork/mocha-utils": "^2.1.1",
    "@types/chai": "^4.3.0",
    "@types/diff": "^5.0.2",
    "@types/ejs": "^3.1.0",
    "@types/mocha": "^9.1.0",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^17.0.14",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "chai": "^4.3.6",
    "colorette": "^2.0.16",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "diff": "^5.0.0",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.1",
    "eslint-plugin-mocha": "^10.0.3",
    "husky": "^7.0.4",
    "mocha": "^9.2.0",
    "mock-fs": "^5.1.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "eta": "^1.12.3",
    "@xmldom/xmldom": "^0.8.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
