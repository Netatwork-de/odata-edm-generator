{
  "name": "@netatwork/odata-edm-generator",
  "version": "0.0.1",
  "description": "OData EDM generator",
  "typings": "dist/types/decorators",
  "main": "dist/cjs/api/decorators.js",
  "module": "dist/esm/decorators.js",
  "bin": {
    "gen-edm": "dist/cjs/cli/index.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts --cache",
    "prebuild": "rimraf dist && npm run lint",
    "build": "tsc && tsc -p tsconfig.esm.json",
    "dev": "tsc -w --preserveWatchOutput",
    "test": "nyc --nycrc-path ./tests/nyc.config.js mocha --config ./tests/.mocharc.js",
    "test:dev": "cross-env DEV=true nyc --nycrc-path ./tests/nyc.config.js mocha --config ./tests/.mocharc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Netatwork-de/odata-edm-generator.git"
  },
  "keywords": [
    "odata",
    "edm",
    "generator"
  ],
  "author": "Net at Work GmbH",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Netatwork-de/odata-edm-generator/issues"
  },
  "homepage": "https://github.com/Netatwork-de/odata-edm-generator#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@netatwork/mocha-utils": "^1.1.5",
    "@types/chai": "^4.2.15",
    "@types/ejs": "^3.0.6",
    "@types/mocha": "^8.2.1",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^14.14.31",
    "@types/pluralize": "0.0.29",
    "@types/xmldom": "^0.1.30",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "chai": "^4.3.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-mocha": "^8.0.0",
    "husky": "^5.1.1",
    "mocha": "^8.3.0",
    "mock-fs": "^4.13.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.19",
    "standard-version": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "xmldom": "^0.4.0"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "pluralize": "^8.0.0",
    "xml-js": "^1.6.11",
    "yargs": "^16.2.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
