<%_
    const tabSize = 4;
    const tabs2 = ' '.repeat(tabSize*2);
    const tabs3 = ' '.repeat(tabSize*3);
_%>
<%_ if (endpoint) { _%>
@odataEndpoint(Endpoints.<%- endpoint %>)
<%_ } _%>
<%_ if (baseType && propertyInfos.length > baseType.propertyInfos.length) { _%>
// @ts-ignore needed to avoid this issue: https://github.com/microsoft/TypeScript/issues/4628
<%_ } _%>
export class <%- className %><%_ if (baseType) { %> extends <%- baseType.className %><%_ } %> {

    public static create<T<%- className %> extends <%- className %> = <%- className %>>(this: Class<T<%- className %>>, model: Partial<T<%- className %>>): T<%- className %> {
        return new this(
            <%_ for(const p of propertyInfos) { _%>
            model.<%- p.name %>,
            <%_ } _%>
        );
    }

    public constructor(
        <%_ for(const p of propertyInfos) { _%>
        public <%- p.name %><%_if(p.isNullable){%>?<%_}_%>: <%- p.type %>,
        <%_ } _%>
    ) <%_ if (baseType) { %> {
        super(
            <%_ for(const p of baseType.propertyInfos) { _%>
            <%- p.name %>,
            <%_ } _%>
        );
    }
    <%_ } else { %> { } <%_ } %>
}